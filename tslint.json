{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prettier": [
      true,
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "none"
      }
    ]
  }
}
